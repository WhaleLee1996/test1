$ mkdir CV && cd CV
$ git init
$ git config --global user.name "Whalelee"
$ git config --global user.email "2513987693@qq.com"
$ wget http://labfile.oss.aliyuncs.com/courses/624/cv-template.zip
$ unzip cv-template
$ mv cv-template/* .
$ rm -rf cv-template* __MACOSX*
$ git add
$ git commit-m 'whalelee cv'
$ git remote add origin https://github.com/WhaleLee1996/test1.git
$ git push -u origin master
